<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
 <display-name>czweb</display-name>
 <listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file> 
  </welcome-file-list>
  <filter>
		<filter-name>etmvc</filter-name>
		<filter-class>com.et.mvc.SpringDispatcherFilter</filter-class>
		<init-param>
			<param-name>controllerBasePackage</param-name>
			<param-value>com.zytx.controllers</param-value>
		</init-param>
		<init-param>
			<param-name>viewBasePath</param-name>
			<param-value>/views</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>etmvc</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name><![CDATA[ywNewImage]]></servlet-name>
		<servlet-class><![CDATA[com.zytx.web.servlet.ReadYwNewImage]]></servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name><![CDATA[ywNewImage]]></servlet-name>
		<url-pattern><![CDATA[/servlet/ywNewImage.jpg]]></url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name><![CDATA[ywImage]]></servlet-name>
		<servlet-class><![CDATA[com.zytx.web.servlet.ReadYwImage]]></servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name><![CDATA[ywImage]]></servlet-name>
		<url-pattern><![CDATA[/servlet/ywImage.jpg]]></url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name><![CDATA[ywImage2]]></servlet-name>
		<servlet-class><![CDATA[com.zytx.web.servlet.ReadAzImage]]></servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name><![CDATA[ywImage2]]></servlet-name>
		<url-pattern><![CDATA[/servlet/ywImage2.jpg]]></url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name><![CDATA[companyImage]]></servlet-name>
		<servlet-class><![CDATA[com.zytx.web.servlet.CompanyImage]]></servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name><![CDATA[companyImage]]></servlet-name>
		<url-pattern><![CDATA[/servlet/companyImage.jpg]]></url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name><![CDATA[ywRateCompanyImage]]></servlet-name>
		<servlet-class><![CDATA[com.zytx.web.servlet.YwRateCompanyImage]]></servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name><![CDATA[ywRateCompanyImage]]></servlet-name>
		<url-pattern><![CDATA[/servlet/ywRateCompanyImage.jpg]]></url-pattern>
	</servlet-mapping>
  <servlet>
   <servlet-name>DisplayChart</servlet-name> 
  <servlet-class>org.jfree.chart.servlet.DisplayChart</servlet-class> 
  </servlet> 
  <servlet-mapping>
   <servlet-name>DisplayChart</servlet-name> 
   <url-pattern>/servlet/DisplayChart</url-pattern> 
   </servlet-mapping>
</web-app>